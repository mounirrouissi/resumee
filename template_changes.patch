+        """Professional template styles - clean and ATS-friendly."""
+        from reportlab.lib.styles import getSampleStyleSheet
+        
+        base_styles = getSampleStyleSheet()
+        
+        return {
+            'name': ParagraphStyle(
+                'ATSName',
+                parent=base_styles['Normal'],
+                fontName='Helvetica-Bold',
